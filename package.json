{
  "name": "relm4-snippets",
  "publisher": "relm4",
  "displayName": "Relm4 Snippets",
  "description": "Relm4 code snippets",
  "version": "0.0.1",
  "icon": "images/Relm_logo.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Relm4/vscode-relm4-snippets.git"
  },
  "keywords": [
    "Relm",
    "Relm4",
    "GTK",
    "Adwaita",
    "Libadwaita",
    "Rust"
  ],
  "scripts": {
    "package": "npx vsce package",
    "publish": "npx vsce publish",
    "open-in-browser": "vscode-test-web --extensionDevelopmentPath=. ."
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "rust",
        "path": "./snippets/components.json"
      },
      {
        "language": "rust",
        "path": "./snippets/factories.json"
      },
      {
        "language": "rust",
        "path": "./snippets/templates.json"
      },
      {
        "language": "rust",
        "path": "./snippets/workers.json"
      }
    ]
  }
}
